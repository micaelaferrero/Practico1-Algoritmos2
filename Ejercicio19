Metodo para sumar dos matrices y poner en la tercera los resultados

void sumaMat(int A[Max][Max],int B[Max][Max],int n,int m)
{int C[Max][Max];
   for(int i=0;i<n;i++)
   { for(int j=0;j<n;j++)
      { 
          C[i][j]=A[i][j]+B[i][j];
      }
   }
   for(int i=0;i<n;i++)
   { for(int j=0;j<n;j++)
      {
          cout<<"La matriz resultante es :"<<C[i][j];
      }
   }
}
